<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" xmlns:Custom="http://www.galasoft.ch/mvvmlight" x:Class="rupload.MainWindow"
        Title="Rupload" Height="350" Width="525" WindowStyle="ToolWindow"
        DataContext="{Binding Main, Source={StaticResource Locator}}">
	<i:Interaction.Triggers>
		<i:EventTrigger>
			<Custom:EventToCommand Command="{Binding UploadCommand, Mode=OneWay}"/>
		</i:EventTrigger>
	</i:Interaction.Triggers>
	<Grid/>
</Window>
